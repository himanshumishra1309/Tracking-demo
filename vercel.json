{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/css/styles.css",
        "use": "@vercel/static"
      },
      {
        "src": "public/js/script.js",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/css/(.*)",
        "dest": "/public/css/$1"
      },
      {
        "src": "/js/(.*)",
        "dest": "/public/js/$1"
      },
      {
        "src": "/(.*)",
        "dest": "app.js"
      }
    ]
  }
  